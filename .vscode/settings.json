{
    "breadcrumbs.enabled": true,
    "cSpell.ignoreRegExpList": [
        "CStyleComment",
    ],
    "cSpell.words": [
        "abiword",
        "alnum",
        "ALPHABRACES",
        "avif",
        "BLACKSLASH",
        "checkstyle",
        "cntrl",
        "codeframe",
        "commitlint",
        "contenturl",
        "devserver",
        "disableable",
        "downloadurl",
        "duringmedia",
        "elevatable",
        "embedurl",
        "epub",
        "fullhash",
        "Hashcode",
        "Hitallo",
        "Hookable",
        "hoverable",
        "htmlx",
        "iconfont",
        "IHTMLX",
        "imagesrcset",
        "inputable",
        "installurl",
        "itemprop",
        "jfif",
        "labelable",
        "layoutimage",
        "libnpmpack",
        "libnpmpublish",
        "minipass",
        "mpkg",
        "msvideo",
        "msword",
        "myme",
        "NUMBRACES",
        "onsearch",
        "opendocument",
        "pacote",
        "pathinfo",
        "precomposed",
        "prefetch",
        "publishignore",
        "punct",
        "quasis",
        "queryfy",
        "reobserve",
        "rippleable",
        "Routeable",
        "tapable",
        "themeable",
        "thumbnailurl",
        "tileimage",
        "truetype",
        "typescrypt",
        "UIID",
        "visio",
        "weba",
        "webcomponents",
        "xdigit",
        "xlink"
    ],
    "editor.roundedSelection": false,
    "files.associations": {
        "*.config": "xml",
        "*.resx": "xml"
    },
    "files.exclude": {
        "**/*.config": true,
        "**/*.orig": true,
        "**/*.tsbuildinfo": true,
        "**/.DS_Store": true,
        "**/.commitlintrc.json": true,
        "**/.editorconfig": true,
        "**/.eslintcache": true,
        "**/.eslintignore": true,
        "**/.eslintrc.json": true,
        "**/.git": true,
        "**/.gitignore": true,
        "**/.gitmodules": true,
        "**/.hg": true,
        "**/.nvmrc": true,
        "**/.nyc_output": true,
        "**/.nycrc.json": true,
        "**/.svn": true,
        "**/.vs": true,
        "**/CVS": true,
        "**/LICENSE": true,
        "**/coverage": true,
        "**/node_modules": true
    },
    "javascript.format.placeOpenBraceOnNewLineForControlBlocks": true,
    "javascript.format.placeOpenBraceOnNewLineForFunctions": true,
    "javascript.preferences.quoteStyle": "double",
    "js/ts.implicitProjectConfig.checkJs": true,
    "typescript.format.placeOpenBraceOnNewLineForControlBlocks": true,
    "typescript.format.placeOpenBraceOnNewLineForFunctions": true,
    "typescript.preferences.quoteStyle": "double",
    "typescript.tsdk": "modules/node_modules/typescript/lib",
    "vsicons.associations.folders": [
        {
            "extensions": [
                "internal"
            ],
            "format": "svg",
            "icon": "private"
        },
        {
            "extensions": [
                "tasks"
            ],
            "format": "svg",
            "icon": "tools"
        }
    ],
    "workbench.iconTheme": "vscode-icons",
    "cSpell.enabled": true
}
