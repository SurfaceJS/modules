{
    "breadcrumbs.enabled": true,
    "cSpell.ignoreRegExpList": [
        "CStyleComment",
    ],
    "cSpell.words": [
        "AGMSY",
        "alnum",
        "ALPHABRACES",
        "cntrl",
        "commitlint",
        "ekqw",
        "Hitallo",
        "htmlx",
        "iconfont",
        "libnpmpack",
        "libnpmpublish",
        "minipass",
        "NUMBRACES",
        "pacote",
        "punct",
        "tapable",
        "typescrypt",
        "webcomponents",
        "xdigit"
    ],
    "editor.roundedSelection": false,
    "files.associations": {
        "*.config": "xml",
        "*.resx": "xml"
    },
    "files.exclude": {
        "**/*.config": false,
        "**/*.orig": false,
        "**/*.tsbuildinfo": false,
        "**/.DS_Store": false,
        "**/.commitlintrc.json": false,
        "**/.editorconfig": false,
        "**/.eslintcache": false,
        "**/.eslintignore": false,
        "**/.eslintrc.json": false,
        "**/.git": false,
        "**/.gitignore": false,
        "**/.gitmodules": false,
        "**/.hg": false,
        "**/.nvmrc": false,
        "**/.nyc_output": false,
        "**/.nycrc.json": false,
        "**/.svn": false,
        "**/.vs": false,
        "**/CVS": false,
        "**/LICENSE": false,
        "**/coverage": false,
        "**/node_modules": false
    },
    "javascript.format.placeOpenBraceOnNewLineForControlBlocks": true,
    "javascript.format.placeOpenBraceOnNewLineForFunctions": true,
    "javascript.preferences.quoteStyle": "double",
    "js/ts.implicitProjectConfig.checkJs": true,
    "typescript.format.placeOpenBraceOnNewLineForControlBlocks": true,
    "typescript.format.placeOpenBraceOnNewLineForFunctions": true,
    "typescript.preferences.quoteStyle": "double",
    "typescript.tsdk": "modules/node_modules/typescript/lib",
    "vsicons.associations.folders": [
        {
            "extensions": [
                "internal"
            ],
            "format": "svg",
            "icon": "private"
        },
        {
            "extensions": [
                "tasks"
            ],
            "format": "svg",
            "icon": "tools"
        }
    ],
    "workbench.iconTheme": "vscode-icons"
}
