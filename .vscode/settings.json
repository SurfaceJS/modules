        "hashcode",
        "hitallo",
        "hookable",
        "hoverable",
        "htmlx",
        "iconfont",
        "ihtmlx",
        "iife",
        "imagesrcset",
        "inputable",
        "installurl",
        "isnan",
        "itemprop",
        "jfif",
        "labelable",
        "layoutimage",
